	Issn = {1748-9326},
	Journal = {Environmental Research Letters (submitted)},
	Keywords = {budburst,downscaled climate model,hard freeze,late freeze,phenological mismatch,spring index,spring onset},
	Number = {October},
	Pages = {104008},
	Publisher = {IOP Publishing},
	Title = {{Spring plant phenology and false springs in the conterminous U. S. during the 21st century}},
	Volume = {10},
