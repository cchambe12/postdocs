	Keywords = {Freezing tolerance,Growth rate,Live oaks,Quercus series Virentes,Trade-off,Tropical-temperate divide},
	Number = {3},
	Pages = {730--744},
	Pmid = {22171967},
	Title = {{Evidence for a freezing tolerance-growth rate trade-off in the live oaks (Quercus series Virentes) across the tropical-temperate divide}},
