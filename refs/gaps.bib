	Doi = {doi:10.1046/j.1365-2486.1998.t01-1-00168.x},
	Isbn = {1354-1013},
	Journal = {Global Change Biology},
	Keywords = {biosphere--atmosphere interactions; climate--vegetation feedback; coupled models},
	Number = {5},
	Pages = {561--579},
	Title = {Coupling dynamic models of climate and vegetation},
	Ty = {JOUR},
	Volume = {4},
	Year = {1998},
	Bdsk-Url-1 = {https://doi.org/10.1046/j.1365-2486.1998.t01-1-00168.x},
