	Isbn = {0048-9697},
	Journal = {Science of The Total Environment},
	Keywords = {Climate change; Herbivory; Pathogens; Forest epidemiology; Risk assessment; Disturbance},
	Number = {3},
	Pages = {263--286},
	Title = {Assessing the consequences of global change for forest disturbance from herbivores and pathogens},
