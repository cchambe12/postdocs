	Issn = {0378-1127},
	Journal = {Forest Ecology and Management},
	Keywords = {Soil microbial community, PLFA, Northern hardwoods, Wisconsin, Group selection harvest},
	Pages = {82 - 94},
	Title = {Soil microbial community response and recovery following group selection harvest: Temporal patterns from an experimental harvest in a US northern hardwood forest},
	Url = {http://www.sciencedirect.com/science/article/pii/S0378112714007191},
