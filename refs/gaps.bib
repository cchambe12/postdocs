	Pages = {110},
	Publisher = {Frontiers Media S.A.},
	Title = {{Patterns of late spring frost leaf damage and recovery in a European beech (\textit{Fagus sylvatica L.}) stand in south-eastern Germany based on repeated digital photographs}},
	Ty = {JOUR},
