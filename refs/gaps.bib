	Issn = {0378-1127},
	Journal = {Forest Ecology and Management},
	Keywords = {Canopy accession, Canopy release, Oaks, Radial growth averaging, Central hardwood forest},
	Number = {1},
	Pages = {285 - 297},
	Title = {Oak establishment and canopy accession strategies in five old-growth stands in the central hardwood forest region},
	Url = {http://www.sciencedirect.com/science/article/pii/S0378112703001555},
	Volume = {184},
	Year = {2003},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0378112703001555},
	Bdsk-Url-2 = {https://doi.org/10.1016/S0378-1127(03)00155-5}}

@article{Rentch2003,
	Abstract = {{Using a radial growth averaging technique, decadal-scale growth changes of 150- to 350-yr-old overstory oaks were used to identify canopy disturbance events in five old-growth stands, and canopy disturbance intervals were estimated using frequency analysis. Stem maps and tree-establishment and canopy-accession chronologies were then used to reconstruct the size of canopy openings. Between 1700 and 1990, median canopy disturbance interval was approximately 3 yr for each stand; larger disturbances involving two or more trees occurred about every 16 yr per stand. Based on age distribution of the oldest cohorts, three stands became established after a major disturbance, but the majority of multiple-tree disturbances were associated with gaps \\&lt; 200 m2 in area. Historic disturbance frequency and size distribution of canopy gaps suggest that the oak components of these stands persisted by utilizing a variety of growth strategies appropriate to large and small openings. The absence of significant changes in overstory disturbance frequencies further suggests that increases in the level of understory competition are responsible for the present-day decline in oak dominance.}},
	Author = {Rentch, James S. and Fajvan, Mary Ann and Hicks, Ray R., Jr.},
