	Issn = {0829-318X},
	Journal = {Tree Physiology},
	Month = {04},
	Number = {4},
	Pages = {541-550},
	Title = {{Morphological and physiological responses of Scots pine fine roots to water supply in a dry climatic region in Switzerland}},
